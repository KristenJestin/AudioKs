<Application x:Class="AudioKs.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AudioKs"
             Startup="Application_Startup">
    <!--StartupUri="MainWindow.xaml"-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <!--Style-->
                <ResourceDictionary Source="Dictionaries/Style/Style.Default.xaml" />
                <!--Lang-->
                <ResourceDictionary Source="Dictionaries/Lang/Strings.en.xaml" />
                
                <!--Default Value-->
                <ResourceDictionary Source="Dictionaries/DefaultSettings.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
